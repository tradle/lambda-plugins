{
  "exclude": ["./sls-aws-example", "./cjs", "./mjs"],
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "strict": true,
    "module": "es2020",
    "strictFunctionTypes": true,
    "forceConsistentCasingInFileNames": true,
    "target": "ES2017",
    "moduleResolution": "node",
    "declaration": true,
    "lib": ["es2018"],
    "outDir": "./mjs",
  }
}
